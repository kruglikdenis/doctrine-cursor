{
  "name": "munkie/doctrine-cursor",
  "type": "project",
  "license": "MIT",
  "description": "Paginatable cursor implementation for Doctrine ORM",
  "autoload": {
    "psr-4": {
      "Mnk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mnk\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "doctrine/orm": "~2.5",
    "knplabs/knp-components": "^1.3",
    "phpunit/phpunit": "^6.0",
    "symfony/event-dispatcher": "^4.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
